{
  "name": "bfx-hf-ui",
  "version": "1.0.0",
  "engines": {
    "node": ">=6"
  },
  "main": "build/index.html",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --importer=node_modules/node-sass-tilde-importer",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --importer=node_modules/node-sass-tilde-importer",
    "start-server": "node scripts/start-server.js",
    "start": "react-scripts start",
    "dev": "concurrently --kill-others \"npm run start-server\" \"npm run start\"",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js"
  },
  "author": "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bitfinexcom/honey-framework-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/honey-framework-ui.git"
  },
  "keywords": [
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "devDependencies": {
    "node-sass-tilde-importer": "^1.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "@blueprintjs/core": "3.1.0",
    "@blueprintjs/datetime": "3.1.0",
    "@blueprintjs/select": "3.1.0",
    "axios": "^0.18.0",
    "bfx-hf-indicators": "bitfinexcom/bfx-hf-indicators",
    "bfx-hf-util": "bitfinexcom/bfx-hf-util",
    "bignumber.js": "^7.2.1",
    "bitfinex-api-node": "^2.0.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.39.2",
    "color-scheme": "^1.0.1",
    "d3": "^5.4.0",
    "lodash": "^4.17.11",
    "node-sass-chokidar": "^1.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.4.0",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.1.4",
    "react-stockcharts": "^0.7.7",
    "react-transition-group": "^2.3.1",
    "react-virtualized": "^9.19.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  }
}
