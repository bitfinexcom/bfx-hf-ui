{
  "name": "bfx-hf-ui",
  "version": "2.1.0",
  "engines": {
    "node": ">=6"
  },
  "main": "./build/electron.js",
  "homepage": "./",
  "build": {
    "productName": "The Honey Framework",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/**/*",
      "scripts/**/*"
    ],
    "asar": false,
    "appId": "com.bfx-hf-ui",
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "icon": "build/icon.png"
    },
    "win": {
      "icon": "build/icon.png"
    },
    "linux": {
      "icon": "build/icon.png"
    }
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --importer=node_modules/node-sass-tilde-importer",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --importer=node_modules/node-sass-tilde-importer",
    "start-server": "node scripts/start-server.js",
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "electron-dev": "electron .",
    "electron-prod": "nohup electron . > logs.log",
    "dist": "build --mac --win --linux -c.extraMetadata.main=build/electron.js --publish never"
  },
  "author": "Bitfinex",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bitfinexcom/honey-framework-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/honey-framework-ui.git"
  },
  "keywords": [
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "devDependencies": {
    "cjs": "0.0.11",
    "cjs-loader": "^0.1.0",
    "concurrently": "^4.1.0",
    "electron": "4.2.4",
    "electron-builder": "20.41.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "node-sass-tilde-importer": "^1.0.2",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "@blueprintjs/core": "3.1.0",
    "@blueprintjs/datetime": "3.1.0",
    "@blueprintjs/select": "3.1.0",
    "axios": "^0.18.0",
    "bfx-hf-algo": "git+https://github.com/bitfinexcom/bfx-hf-algo.git",
    "bfx-hf-algo-server": "git+https://github.com/bitfinexcom/bfx-hf-algo-server.git",
    "bfx-hf-ext-plugin-bitfinex": "git+https://github.com/bitfinexcom/bfx-hf-ext-plugin-bitfinex.git",
    "bfx-hf-indicators": "bitfinexcom/bfx-hf-indicators",
    "bfx-hf-models": "git+https://github.com/bitfinexcom/bfx-hf-models.git",
    "bfx-hf-models-adapter-lowdb": "git+https://github.com/bitfinexcom/bfx-hf-models-adapter-lowdb.git",
    "bfx-hf-server": "https://github.com/bitfinexcom/bfx-hf-server.git",
    "bfx-hf-util": "bitfinexcom/bfx-hf-util",
    "bignumber.js": "^7.2.1",
    "bitfinex-api-node": "^2.0.2",
    "child_process": "^1.0.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.39.2",
    "color-scheme": "^1.0.1",
    "cors": "^2.8.5",
    "d3": "^5.4.0",
    "dotenv": "^8.0.0",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "express-request-proxy": "^2.2.2",
    "lodash": "^4.17.11",
    "node-sass-chokidar": "^1.3.0",
    "nohup": "^0.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.4.0",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.4.0",
    "react-modal": "^3.9.1",
    "react-monaco-editor": "^0.26.2",
    "react-notifications": "^1.4.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.1.4",
    "react-stockcharts": "^0.7.7",
    "react-switch": "^5.0.0",
    "react-transition-group": "^2.3.1",
    "react-virtualized": "^9.19.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "socks-proxy-agent": "^4.0.2",
    "url": "^0.11.0",
    "wait-on": "^3.2.0"
  }
}
