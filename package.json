    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "dependencies": {
    "@projectstorm/react-diagrams": "^5.3.2",
    "@ufx-ui/bfx-containers": "0.8.7",
